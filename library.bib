journal = {Springer Ser. Fluoresc.},
keywords = {3-hydroxychromones {\'{a}} lipid domains,apoptosis {\'{a}} fluorescence imaging,polarity and,{\'{a}},{\'{a}} fluorescent membrane probes,{\'{a}} membrane electrostatics {\'{a}}},
