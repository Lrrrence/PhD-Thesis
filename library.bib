issn = {00207683},
journal = {Int. J. Solids Struct.},
keywords = {Constitutive behavior,High-rate,Polymer,Split-Hopkinson},
number = {5},
pages = {1331--1356},
title = {{Mechanics of the rate-dependent elastic-plastic deformation of glassy polymers from low to high strain rates}},
volume = {43},
year = {2006}
}
