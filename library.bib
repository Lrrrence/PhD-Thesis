@phdthesis{Feist2001,
abstract = {In the future gas turbines and aero engine designers will try to improve efficiency and reduce emission of pollutants, in particular NOx. The former is and will continue to be achieved by an increase of the turbine entry temperature. To ensure the integrity of the components, which are exposed to these temperatures, novel cooling schemes are being developed and ceramic protective coatings are employed to reduce the thermal load. However, air is also needed for the combustion processes in order to reduce emissions, so that the amount of air available for cooling is limited. To make most effective use of the cooling air a detailed knowledge is required of the temperature distribution on the components of interest. Surface temperature measurements in operating engines are only possible using a few techniques. Commonly used examples are thermocouples, thermal paints or pyrometry. Relatively new is the application of phosphorescent materials to remotely detect temperatures on turbine components and even on rotating blades, combustor walls or on reacting solid propellants. The method involves excitation of a thin layer of material with UV light and observation of the subsequent phosphorescence in the visible part of the spectrum. Some of the phosphorescent properties are temperature dependent such as lifetime decay, intensity of the emission lines and the shift of emission lines. The calibration of these properties can provide a tool for temperature detection. This method is called phosphor thermometry. It was intensively employed for gas turbine applications in the United States during the 1980s and 1990s but was less applied in Europe. In the present study the reader will be introduced to some of the physical fundamentals of phosphor luminescence and the utilisation of the optical properties for temperature measurement. A database of thermographic phosphors has been generated for temperatures in the range from room temperature to 1400oC the latter being believed to be the maximum temperature achievable with this technique. A new previously unknown thermographic phosphor was identified and studied. The results were published in [1] and also now form a part of a global database of rare earths at the ‘Rare-earth Information Center' Ames Laboratory. During the project, a novel concept was introduced that combines thermal barrier protective coatings with the luminescence properties of phosphors. This concept is called ‘smart coating' or ‘thermal barrier sensor coating' and is currently the subject of an international patent application which has already been identified as a ‘patentable invention' by the European Patent Office in December 2000 [2]. Prototype coatings have been fabricated and successfully tested for temperature sensitive properties. It has proven possible to detect subsurface temperatures and there were also indications that structural changes inside the coatings can be measured, a feature which might be used for lifetime assessments of coatings during operation in the future. A thermographic phosphor was used to measure the surface temperature distribution inside a Rolls-Royce research combustor. The combustor was operated at atmospheric pressure and is of the type, which is used in helicopter engines. A temperature profile of a target area in the central part of the combustor was obtained with a spatial resolution of 0.7mm and covering an area of 7x7.5mm2. Ejection holes were clearly identified as heat sinks revealing temperatures between 300oC and 350oC with hot areas showing temperatures between 550oC and 600oC located between the cooling holes. The line-of-sight was directed through the flame of the combustor, which was running under take-off conditions. This ability to measure surface temperatures in the presence of flames is perhaps the most substantial difference between thermographic phosphor thermometry and other optical techniques.},
author = {Feist, J. P.},
file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/Books, Papers, Etc/Thesis/Feist (2001) PhD Thesis - Development of phosphor thermometry for gas turbines.pdf:pdf},
number = {June},
school = {University of London},
title = {{Development of phosphor thermometry for gas turbines}},
year = {2001}
}
@article{Barshan2000,
abstract = {Four methods of range measurement for airborne ultrasonic systems - namely simple thresholding, curve-fitting, sliding-window, and correlation detection - are compared on the basis of bias error, standard deviation, total error, robustness to noise, and the difficulty/complexity of implementation. Whereas correlation detection is theoretically optimal, the other three methods can offer acceptable performance at much lower cost. Performances of all methods have been investigated as a function of target range, azimuth, and signal-to-noise ratio. Curve fitting, sliding window, and thresholding follow correlation detection in the order of decreasing complexity. Apart from correlation detection, minimum bias and total error is most consistently obtained with the curve-fitting method. On the other hand, the sliding-window method is always better than the thresholding and curve-fitting methods in terms of minimizing the standard deviation. The experimental results are in close agreement with the corresponding simulation results. Overall, the three simple and fast processing methods provide a variety of attractive compromises between measurement accuracy and system complexity. Although this paper concentrates on ultrasonic range measurement in air, the techniques described may also find application in underwater acoustics.},
author = {Barshan, Billur},
doi = {10.1088/0957-0233/11/1/307},
file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/LYule PhD Study Shared/ToF Methods/mst00-1.pdf:pdf},
issn = {09570233},
journal = {Meas. Sci. Technol.},
keywords = {Correlation detection,Range measurement,Sliding window,Sonar,Target localization and identification,Thresholding,Time-of-flight measurement,Ultrasonics},
number = {1},
pages = {45--50},
title = {{Fast processing techniques for accurate ultrasonic range measurements}},
volume = {11},
year = {2000}
}
