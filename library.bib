issn = {15734897},
journal = {Appl. Compos. Mater.},
keywords = {Circular damage,Defect imaging,Full Matrix Capture,Image reconstruction,Lamb waves,Lamb waves reflections,Signal processing,Structural Health Monitoring,Symmetric fundamental mode,Total Focusing Method},
number = {2},
pages = {553--573},
publisher = {Applied Composite Materials},
title = {{Structural Health Monitoring Using Lamb Wave Reflections and Total Focusing Method for Image Reconstruction}},
