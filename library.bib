file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/Books, Papers, Etc/Ultrasound Methods/SAW/sensors-19-03077.pdf:pdf},
issn = {1424-8220},
journal = {Sensors},
keywords = {1,2,45 ghz,advantages in industrial sensing,and can sustain high,as they can be,delay lines,industrial application,introduction to saw sensors,it has been,readout wirelessly,saw,sensors,sensors can provide significant,surface acoustic wave,temperatures,wireless,work passively},
