@article{Ji2019,
abstract = {Smart sensors are necessary to measure the surface temperature of the key aero-engine components such as nozzle guide vane – one of the key parts receiving the highest heat after the gas combustion. Traditional sensors such as wired thermocouple (TC) are cumbersome to implant into the system due to its large size and hard installment. In this article simple yet effective TC array was formed by a series of Pt soldering dots on the PtRh thin film on the thermal barrier coating (TBC) on turbine blade substrate. The PtRh thin film was fabricated using MEMS technology to pattern thermocouple on the curved surface of nozzle guide vane. Heating experiments were performed using ethanol and acetylene flame, calibration is carried out in muffle furnace. The distribution of the measured thermal voltage from the sensor array agrees with temperature distribution caused by the ethanol heating flame responsively. Temperature cycling tests were also performed to test the robustness of such sensors. In addition other types of smart sensors were also formulated and tested: Pt/PtRh sensor with two layers of Pt and PtRh thin films overlapping into thermocouple joint on TBC and simple dot-like sensors using YSZ TBC as a thermistor to monitor the surface temperature on the TBC coating. The advantage of the former sensor is the minimum turbulence to the heat flows on the vane surface while for the latter an easy economical way to monitor the surface temperature on the turbine blade.},
author = {Ji, Zhonglin and Weng, Haotian and Hu, Mingkai and Duan, Franklin Li and Li, Jie},
doi = {10.2514/6.2019-4084},
file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/LYule PhD Study Shared/Gas turbine sensing/Thermocouples/Ji (2019) Temp distribution measurements on turbine blade surface.pdf:pdf},
isbn = {9781624105906},
journal = {AIAA Propuls. Energy Forum Expo. 2019},
number = {August},
pages = {10--13},
title = {{Temperature distribution measurements on turbine blade surface by the aid of simple dotted Pt/PtRh thermal couple test array}},
volume = {1},
year = {2019}
}
@article{Yamazaki2007,
abstract = {Thermal barrier coatings (TBCs), that reduce the temperature in the underlying substrate material, are an essential requirement for the hot section components of industrial gas turbines. Recently, in order to take full advantage of the potential of the TBC systems, experimental and analytical investigations in TBC systems have been performed. However there is a little information on the deformation behavior of the top coating. In addition, the effects of the thermal exposure and the process parameters on the mechanical properties of the top coating have never been clarified. From these backgrounds, the effects of the process variables in APS and the thermal exposure on the mechanical properties were investigated in order to optimize the APS process of top coatings. The experimental results indicated that the mechanical properties of the APS-TBC, i.e. the tensile strength and the elastic modulus, were significantly changed by the process variables and the long term thermal exposure. The microstructural investigation was also carried out and the relationship between the mechanical properties and the porosity was discussed.},
