file = {:C$\backslash$:/Users/yulel/OneDrive - University of Southampton/LYule PhD Study Shared/Gas turbine sensing/Thermal Paints {\&} Phosphors/GT2012-69811.pdf:pdf},
isbn = {9780791844670},
journal = {Proc. ASME Turbo Expo},
keywords = {GT2012-69811},
pages = {927--933},
title = {{Phosphor based temperature indicating paints}},
