keywords = {Laser-induced phosphorescence,Phosphor thermometry,Remote temperature sensing,Thermal barrier coatings,Thermographic phosphors},
pages = {359--367},
publisher = {Elsevier B.V.},
title = {{Remote temperature sensing on and beneath atmospheric plasma sprayed thermal barrier coatings using thermographic phosphors}},
url = {http://dx.doi.org/10.1016/j.surfcoat.2016.06.038},
volume = {302},
year = {2016}
