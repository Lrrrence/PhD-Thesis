journal = {IEEE Int. Ultrason. Symp. IUS},
keywords = {Heat Flux,Industrial Applications,Temperature Distribution,Ultrasonic Sensors},
pages = {940--943},
publisher = {IEEE},
title = {{Ultrasonic measurements of temperature distribution and heat fluxes across containments of extreme environments}},
volume = {2019-Octob},
year = {2019}
}
