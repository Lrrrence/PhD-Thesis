@article{Feder1958,
abstract = {Dilatometric and x-ray measurements of the thermal expansion of pure lead and aluminum have been carried out between room temperature and the melting point. For lead, the results obtained by the two techniques agree within experimental error, which is interpreted to imply that the vacancy concentration at the melting point is (in mole fraction) less than or equal to 1.5×10-4. For aluminum the dilatometric expansion appears to be slightly greater than the x-ray expansion. If this discrepancy is real, it corresponds to a vacancy concentration at the melting point of aluminum of about 3×10-4. The corresponding estimate for the formation energy, v, of a vacancy in lead is v0.53 ev, and in aluminum, v=0.77 ev. {\textcopyright} 1958 The American Physical Society.},
