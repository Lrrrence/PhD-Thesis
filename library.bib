journal = {IET Conf. Publ.},
keywords = {Luminescence,Non-destructive evaluation,Thermal barrier coatings,Thermal history sensors},
number = {630 CP},
pages = {1--8},
title = {{Thermal barrier sensor coatings - Sensing damage and ageing in critical components}},
volume = {2014},
year = {2014}
}
