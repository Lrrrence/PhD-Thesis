      \field{journaltitle}{Sensors (Switzerland)}
      \field{number}{14}
      \field{title}{{A review of acoustic impedance matching techniques for piezoelectric sensors and transducers}}
      \field{volume}{20}
      \field{year}{2020}
      \field{pages}{1\bibrangedash 65}
      \range{pages}{65}
